/* ---------- toolbar ---------- */


div#toolbar_holder.active {
	background-image: url(/kmod/static/img/ajax-loader.gif);
	background-position:7px 43px;
	background-repeat:no-repeat;
}

#toolbar{
	border-top:1px solid #333;
	//background: url(/kmod/static/img/prototype/toolbar_bg.png);
	background-color:#333;
	//background-color:#6633CC;
}
#toolbar .divider{
	width:11px;
	background: url(/kmod/static/img/prototype/toolbar_div.png) no-repeat;
	height:20px;
}
.navleft {
	float:left;
	margin:8px 0px 8px 20px;
	color:#bbb;
	width:420px;
}
.navleft li{
	display: inline;
	list-style-type: none;
	float: left;
}
.navleft a {
	text-decoration: none;
	color:#ccc;
	padding:5px 8px 4px 8px;
}
.navleft a:hover {
	color:#fff;
}
.navleft a.on{
	color:#fff;
	border:1px solid #aaa;
	padding:4px 7px 3px 7px;
}

#navright{
	margin:6px 20px 2px 0px;
	float:right;
	width:540px;
}
#navright .link{
	float:right;
	margin-top:1px;
}
#navright a{
	text-decoration: none;
	color:#ccc;
	margin:0px 0px 4px 8px;
}
#toolbar .tbutton{
	padding:1px 6px 1px 6px;
	margin:0px 0px 0px 6px;
	border:1px solid #555;
	color:#ccc;
	background:url(/kmod/static/img/prototype/toolbar_but_bg.png);
	height:20px;
	cursor:pointer;	
}
#toolbar .tbutton:hover{
	border:1px solid #999;
	color:#fff;
}


#toolbar .tfield{
	border:1px solid #555555;
	//border-top:1px solid #333;
	//border-left:1px solid #333;
	font-size:100%;
	padding:2px 1px 2px 1px;
	margin:0px 0px 4px 6px;
	background:#333;
	color:#888;
}
#toolbar .tfield:focus{
	border:1px solid #888;
	border-top:1px solid #555;
	border-left:1px solid #555;
	background:#555;
}
