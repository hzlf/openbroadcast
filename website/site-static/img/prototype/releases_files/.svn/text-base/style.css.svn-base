	margin:25px 0px 17px 0px;
	
}
.forumcontent .title{
	font-weight:bold;
	color:#00BB00;
}
.fci{
	
}
.fci .title{
	font-weight:bold;
	color:#000;
}
.fci .title2{
	color:#000;
}
.fci .address{
	font-size:11px;
	color:#999;
}
.exp{
	padding-right:0px !important;
	background-image:url(/kmod/static/img/prototype/box_handler.png);
	background-repeat:no-repeat;
	background-position:0px 2px;
}
.expanded{
	padding-right:0px !important;
	background-image:url(/kmod/static/img/prototype/box_handler_on.png);
	background-repeat:no-repeat;
	background-position:0px 2px;
}
.bold{
	font-weight:bold;
}

/* ---------- basket ---------- */
.boxtitle .count{
	color:#999;
}

.boxsub{
	margin:1px 2px 0px 2px;
	background:#eee;
}
.boxsub-title{
	border:1px solid #bbb;
	//border-top:0px;
	padding:1px 2px 0px 16px;
	background:#ddd url(/kmod/static/img/prototype/box_handler.png) no-repeat -2px -2px;
	cursor:pointer;
}
.boxsub-title.open{
	background-image:url(/kmod/static/img/prototype/box_handler_on.png) !important;
}
.boxsub-content{
	display:none;
	color:#6633CC;
}
.boxsub .b-closer{
	float:right;
	background:url(/kmod/static/img/prototype/basket_remove.png) no-repeat;
	width:17px;height:18px;
}
.boxsub .i-closer{
	float:right;
	background:url(/kmod/static/img/prototype/boxsub_item_remove.png) no-repeat;
	width:17px;height:18px;
}
.boxsub .i-add{
	float:right;
	background:url(/kmod/static/img/prototype/boxsub_item_add.png) no-repeat;
	width:17px;height:18px;
}

.b-closer:hover,.i-closer:hover{
	background:url(/kmod/static/img/prototype/boxsub_remove_on.png) no-repeat !important;
}
.boxsub .b-pl{
	float:right;
	background:url(/kmod/static/img/prototype/basket_to_pl.png) no-repeat;
	width:17px;height:18px;
}
.boxsub .b-play{
	float:right;
	background:url(/kmod/static/img/prototype/list_play_button2_s.png) no-repeat;
	width:17px;height:18px;
}

.boxsub-item{
	padding:2px 2px 1px 16px;
	cursor:pointer;
	border:1px solid #bbb ;
	border-top:1px solid #ccc ;
	margin-top:-1px;
}
.boxsub-item:hover{
	background-color:#e4e4e4 !important;
}
.newbasket{
	display:none;
	border:1px solid #bbb;
	margin:0px 2px 2px 2px;
	padding:3px 3px 3px 15px;
	background:#ddd;
}

.drag{
	position:absolute;
	padding:5px 10px 5px 10px;
	border:1px solid #666;
	background:#eee;
	width:160px;
}
.drop-highlight{
	//border:1px solid #999 !important;
}
.drop-over{
	//border:1px solid #333 !important;
	background-color:#dffddf !important;
}
.boxsub.drop-over .boxsub-title{
	background-color:#c8efc8 !important;
}
.unselectable {
   -moz-user-select: none;
   -khtml-user-select: none;
   user-select: none;
}

.ui-inlineedit{
	color:#6633CC;
	padding:0px 3px 0px 3px;
	margin-right:-3px;
	border:1px dotted #CEBDED;
	//background:#fff;
}
.ui-inlineedit.edit{
	padding:0px 3px 0px 3px;
	border:1px solid #666;
	border-bottom:1px solid #ddd;
	border-right:1px solid #ddd;
	background:#fff;
}

.edit input{
	padding-top:2px;
	margin-bottom:-2px;
	background:none;
	border:0px;
	text-align:right;
}
/* datepicker */
.box #datepicker{
//	border:1px solid #ff00ff;
}
.box .ui-datepicker{
	width:auto;
	margin:2px 2px 2px 2px;
	padding:0px;
	border:0px solid #ff00ff;
	background:none;
}
.box .ui-datepicker-header{
	border:1px solid #bbb !important;
	border-bottom:0px solid #bbb !important;
	margin:0px !important;
}

.box .ui-datepicker-calendar{
	margin:0px !important;
}
.box .ui-datepicker-calendar th{
	border:1px solid #bbb!important;
	padding:0.3em;
	font-size:11px !important;
	padding:3px;
	font-weight:normal;
}

.box .ui-datepicker-calendar td{
	border:1px solid #bbb!important;
	width:14%;
	font-size:11px !important;
	padding:0px;
}
.box .ui-state-default{
	border:0px;
	background:#E4E4F4;
	padding-right:6px;
	color:#6633CC;
}
.box .ui-state-active{
	background:#ddffd9;
	color:#000000;
}



.selectable .ui-selected { background-color: #e4ffe4;} 
.selectable .ui-selected a { color: #555;} 
.selectable .ui-selected:hover { background-color: #e4ffe4; }

.ui-dialog{
	padding:0px;
	border:1px solid #666;
}
.dialog-content{
	padding:10px;
}
.ui-dialog-content{
	padding:0px !important;
}
.ui-widget-header{
	border:0px;
	border-bottom:1px solid #999;
	padding:0px;
	//background:#ccc;
}
.ui-dialog-titlebar{
	padding:3px 0px 0px 7px !important;
	background-color:#666 !important;
	
	
	
}
.ui-dialog-title{
	//text-transform:none !important;
	font-size:11px;
}
.box .ui-widget-header{
	background:#ccc !important;
}
/* pageplayer */

.fadeindiv{
	background:url(/kmod/static/img/prototype/basket_remove.png) no-repeat;
}
